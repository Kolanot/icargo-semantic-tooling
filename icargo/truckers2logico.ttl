# baseURI: http://interop.sensorlab.tno.nl/truckers2logico
# imports: http://interop.sensorlab.tno.nl/def/truckers
# imports: http://ontology.tno.nl/logiserv

@prefix :        <http://interop.sensorlab.tno.nl/truckers2logico#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix to:      <http://interop.sensorlab.tno.nl/def/truckers#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

to:Container
      rdfs:subClassOf <http://ontology.tno.nl/logico#Container> .

to:Driver
      rdfs:subClassOf <http://ontology.tno.nl/logico#Actor> .

to:GeoPosition
      rdfs:subClassOf <http://ontology.tno.nl/logico#GeoPoint> .

to:Organization
      rdfs:subClassOf <http://ontology.tno.nl/logico#Actor> .

to:Terminal
      rdfs:subClassOf <http://ontology.tno.nl/logico#Terminal> .

to:TerminalActivity
      rdfs:subClassOf <http://ontology.tno.nl/logiserv#Activity> .

to:TransportActivity
      rdfs:subClassOf <http://ontology.tno.nl/logiserv#Transport> .

to:Truck
      rdfs:subClassOf <http://ontology.tno.nl/logico#Truck> .

to:container
      rdfs:subPropertyOf <http://ontology.tno.nl/logiserv#involves> .

to:description
      rdfs:subPropertyOf <http://ontology.tno.nl/logico#hasDescription> .

to:email
      rdfs:subPropertyOf <http://ontology.tno.nl/logico#hasEmail> .

to:firstname
      rdfs:subPropertyOf <http://ontology.tno.nl/logico#hasName> .

to:id
      rdfs:subPropertyOf <http://ontology.tno.nl/logico#hasIDValue> .

to:lastname
      rdfs:subPropertyOf <http://ontology.tno.nl/logico#hasName> .

to:latitude
      rdfs:subPropertyOf <http://www.w3.org/2003/01/geo/wgs84_pos#lat> .

to:longitude
      rdfs:subPropertyOf <http://www.w3.org/2003/01/geo/wgs84_pos#long> .

to:name
      rdfs:subPropertyOf <http://ontology.tno.nl/logico#hasName> .

to:position
      rdfs:subPropertyOf <http://www.w3.org/2003/01/geo/wgs84_pos#location> .

to:status
      rdfs:subPropertyOf <http://ontology.tno.nl/logico#hasStatus> .

to:terminal
      rdfs:subPropertyOf <http://ontology.tno.nl/logiserv#involves> .

to:truck
      rdfs:subPropertyOf <http://ontology.tno.nl/logiserv#involves> .

<http://interop.sensorlab.tno.nl/truckers2logico>
      rdf:type owl:Ontology ;
      owl:imports <http://interop.sensorlab.tno.nl/def/truckers> , <http://ontology.tno.nl/logiserv> .
