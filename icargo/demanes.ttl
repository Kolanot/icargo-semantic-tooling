# baseURI: http://interop.sensorlab.tno.nl/demanes

@prefix :        <http://interop.sensorlab.tno.nl/demanes#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<http://interop.sensorlab.tno.nl/demanes>
      rdf:type owl:Ontology .

:Container
      rdf:type owl:Class ;
      rdfs:label "Container "@en .

:Organization
      rdf:type owl:Class ;
      rdfs:label "Organization "@en .

:Terminal
      rdf:type owl:Class .

:TerminalActivity
      rdf:type owl:Class .

:Truck
      rdf:type owl:Class .

:TruckActivity
      rdf:type owl:Class .

:User
      rdf:type owl:Class .

:action
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :TruckActivity ;
      rdfs:label "action "@en ;
      rdfs:range xsd:string .

:activity
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Truck ;
      rdfs:label "activity "@en ;
      rdfs:range :TruckActivity ;
      owl:inverseOf :truck .

:ata  rdf:type owl:DatatypeProperty ;
      rdfs:label "Actual Time of Arrival"@en ;
      rdfs:range xsd:dateTime .

:atd  rdf:type owl:DatatypeProperty ;
      rdfs:label "Actual Time of Departure"@en ;
      rdfs:range xsd:dateTime .

:closingtime
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Terminal ;
      rdfs:label "closingtime "@en ;
      rdfs:range xsd:dateTime .

:container
      rdf:type owl:ObjectProperty ;
      rdfs:domain :TruckActivity ;
      rdfs:label "container "@en ;
      rdfs:range :Container .

:currentactivity
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Truck ;
      rdfs:label "current activity"@en ;
      rdfs:range xsd:string .

:description
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :TruckActivity ;
      rdfs:label "description "@en ;
      rdfs:range xsd:string .

:driver
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Truck ;
      rdfs:label "driver "@en ;
      rdfs:range :User .

:email
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :User ;
      rdfs:label "email "@en ;
      rdfs:range xsd:string .

:eta  rdf:type owl:DatatypeProperty ;
      rdfs:label "Estimated Time of Arrival"@en ;
      rdfs:range xsd:dateTime .

:etd  rdf:type owl:DatatypeProperty ;
      rdfs:label "Estimated Time of Departure"@en ;
      rdfs:range xsd:dateTime .

:firstname
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :User ;
      rdfs:label "firstname "@en ;
      rdfs:range xsd:string .

:fuellevel
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Truck ;
      rdfs:label "fuellevel "@en ;
      rdfs:range xsd:float .

:id   rdf:type owl:DatatypeProperty ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (:Container :Terminal :Truck)
              ] ;
      rdfs:label "id "@en ;
      rdfs:range xsd:string .

:lastname
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :User ;
      rdfs:label "lastname "@en ;
      rdfs:range xsd:string .

:latitude
      rdf:type owl:DatatypeProperty ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (:Terminal :Truck)
              ] ;
      rdfs:label "latitude "@en ;
      rdfs:range xsd:float .

:licensenumber
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Truck ;
      rdfs:label "licensenumber "@en ;
      rdfs:range xsd:string .

:longitude
      rdf:type owl:DatatypeProperty ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (:Terminal :Truck)
              ] ;
      rdfs:label "longitude"@en ;
      rdfs:range xsd:float .

:name
      rdf:type owl:DatatypeProperty ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (:Organization :Terminal)
              ] ;
      rdfs:label "name "@en ;
      rdfs:range xsd:string .

:openingtime
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Terminal ;
      rdfs:label "openingtime "@en ;
      rdfs:range xsd:dateTime .

:organization
      rdf:type owl:ObjectProperty ;
      rdfs:domain :User ;
      rdfs:label "organization "@en ;
      rdfs:range :Organization ;
      owl:inverseOf :users .

:owner
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Truck ;
      rdfs:label "owner "@en ;
      rdfs:range :Organization ;
      owl:inverseOf :trucks .

:status
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :TruckActivity ;
      rdfs:label "status "@en ;
      rdfs:range xsd:string .

:terminal
      rdf:type owl:ObjectProperty ;
      rdfs:domain :TruckActivity ;
      rdfs:label "terminal "@en ;
      rdfs:range :Terminal .

:truck
      rdf:type owl:ObjectProperty ;
      rdfs:domain :TruckActivity ;
      rdfs:label "truck "@en ;
      rdfs:range :Truck ;
      owl:inverseOf :activity .

:trucks
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Organization ;
      rdfs:label "trucks "@en ;
      rdfs:range :Truck ;
      owl:inverseOf :owner .

:username
      rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string .

:users
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Organization ;
      rdfs:label "users "@en ;
      rdfs:range :User ;
      owl:inverseOf :organization .
