@prefix CommonSignatureComponents1:  <urn:oasis:names:specification:ubl:schema:xsd:CommonSignatureComponents#> .
@prefix CommonSignatureComponents2:  <urn:oasis:names:specification:ubl:schema:xsd:CommonSignatureComponents#> .
@prefix SignatureAggregateComponents:  <urn:oasis:names:specification:ubl:schema:xsd:SignatureAggregateComponents#> .
@prefix ccts:    <urn:un:unece:uncefact:documentation:2#> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix dtype:   <http://www.linkedmodel.org/schema/dtype#> .
